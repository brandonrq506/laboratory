{
  "name": "laboratory",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "postinstall": "npm run build",
    "serve-heroku": "npx serve -s dist -l $PORT",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --report-unused-disable-directives --max-warnings 0 --fix",
    "prettier": "prettier --check \"src/**/*.{ts,tsx,html}\"",
    "prettier:fix": "prettier --write \"src/**/*.{ts,tsx,html}\"",
    "preview": "vite preview",
    "test": "vitest",
    "test:expert": "DEBUG_PRINT_LIMIT=10000 npm run test",
    "test:ui": "vitest --ui",
    "type-check": "tsc --noEmit",
    "validate:fix": "npm run prettier:fix && npm run lint:fix && npm run type-check && npm run test"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-query": "^5.81.2",
    "@tanstack/react-query-devtools": "^5.81.2",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.10.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.61.1",
    "react-router": "^7.6.2",
    "serve": "^14.2.4",
    "vite": "^7.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@hookform/devtools": "^4.4.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tanstack/eslint-plugin-query": "^5.81.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/ui": "^3.2.2",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "^19.0.0-beta-714736e-20250131",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-testing-library": "^7.6.1",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "jsdom-testing-mocks": "^1.13.1",
    "msw": "^2.10.4",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.2"
  }
}
