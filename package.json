{
  "name": "laboratory",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "postinstall": "npm run build",
    "serve-heroku": "npx serve -s dist -l $PORT",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --report-unused-disable-directives --max-warnings 0 --fix",
    "prettier": "prettier --check \"src/**/*.{ts,tsx,html}\"",
    "prettier:fix": "prettier --write \"src/**/*.{ts,tsx,html}\"",
    "preview": "vite preview",
    "test": "vitest",
    "test:expert": "DEBUG_PRINT_LIMIT=10000 npm run test",
    "test:ui": "vitest --ui",
    "type-check": "tsc --noEmit",
    "validate:fix": "npm run prettier:fix && npm run lint:fix && npm run type-check && npm run test"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/vite": "^4.1.16",
    "@tanstack/react-devtools": "^0.8.0",
    "@tanstack/react-query": "^5.90.6",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router": "^1.134.13",
    "@tanstack/react-router-devtools": "^1.133.36",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.0",
    "serve": "^14.2.5",
    "vite": "^7.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@hookform/devtools": "^4.4.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@tanstack/eslint-plugin-router": "^1.133.19",
    "@tanstack/router-plugin": "^1.134.12",
    "@testing-library/jest-dom": "^6.9.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-istanbul": "^4.0.8",
    "@vitest/ui": "^4.0.5",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "^19.0.0-beta-714736e-20250131",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-testing-library": "^7.11.0",
    "globals": "^16.5.0",
    "jsdom": "^27.1.0",
    "jsdom-testing-mocks": "^1.16.0",
    "msw": "^2.12.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^4.1.15",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "vite-tsconfig-paths": "^5.1.4",
