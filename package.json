{
  "name": "laboratory",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "postinstall": "npm run build",
    "serve-heroku": "npx serve -s dist -l $PORT",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --report-unused-disable-directives --max-warnings 0 --fix",
    "prettier": "prettier --check \"src/**/*.{ts,tsx,html}\"",
    "prettier:fix": "prettier --write \"src/**/*.{ts,tsx,html}\"",
    "preview": "vite preview",
    "test": "vitest",
    "test:expert": "DEBUG_PRINT_LIMIT=10000 npm run test",
    "test:ui": "vitest --ui",
    "type-check": "tsc --noEmit",
    "validate:fix": "npm run prettier:fix && npm run lint:fix && npm run type-check && npm run test"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/vite": "^4.0.17",
    "@tanstack/react-query": "^5.69.0",
    "@tanstack/react-query-devtools": "^5.71.2",
    "@tanstack/react-table": "^8.21.2",
    "axios": "^1.8.4",
    "clsx": "^2.1.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.54.2",
    "react-router": "^7.4.1",
    "serve": "^14.2.4",
    "vite": "^6.2.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@hookform/devtools": "^4.3.3",
    "@tailwindcss/forms": "^0.5.10",
    "@tanstack/eslint-plugin-query": "^5.68.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-istanbul": "^3.1.1",
    "@vitest/ui": "^3.0.7",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-compiler": "^19.0.0-beta-714736e-20250131",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-testing-library": "^7.1.1",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "msw": "^2.7.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.7"
  }
}